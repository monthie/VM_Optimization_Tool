<Window x:Class="VM_Optimization_Tool.ProgressWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VM_Optimization_Tool"
        mc:Ignorable="d"
        Title="ProgressWindow" Height="100" Width="450" WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
    <Grid>
        <ProgressBar x:Name="progressBar1" HorizontalAlignment="Center" Height="30" Margin="0,0,0,0" VerticalAlignment="Center" Width="400" Minimum="0" Maximum="100"/>
        <TextBlock x:Name="textBlock1" HorizontalAlignment="Center" Margin="0,0,0,0" TextWrapping="Wrap" Text="0%" VerticalAlignment="Center" Height="30" Width="74" FontSize="24" TextAlignment="Center"/>
    </Grid>
</Window>
